{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Config",
  "type": "object",
  "required": [
    "accepted_token",
    "admin",
    "butt",
    "enabled",
    "withdrawal_allowed_from"
  ],
  "properties": {
    "accepted_token": {
      "$ref": "#/definitions/SecretContract"
    },
    "admin": {
      "$ref": "#/definitions/HumanAddr"
    },
    "butt": {
      "$ref": "#/definitions/SecretContract"
    },
    "enabled": {
      "type": "boolean"
    },
    "withdrawal_allowed_from": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "SecretContract": {
      "type": "object",
      "required": [
        "address",
        "contract_hash"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "contract_hash": {
          "type": "string"
        }
      }
    }
  }
}
