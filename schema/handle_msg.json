{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HandleMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "finalize_route"
      ],
      "properties": {
        "finalize_route": {
          "type": "object"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "handle_first_hop"
      ],
      "properties": {
        "handle_first_hop": {
          "type": "object",
          "required": [
            "borrow_amount",
            "hops"
          ],
          "properties": {
            "borrow_amount": {
              "$ref": "#/definitions/Uint128"
            },
            "hops": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Hop"
              }
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "receive"
      ],
      "properties": {
        "receive": {
          "type": "object",
          "required": [
            "amount",
            "from",
            "sender"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint128"
            },
            "from": {
              "$ref": "#/definitions/HumanAddr"
            },
            "msg": {
              "anyOf": [
                {
                  "$ref": "#/definitions/Binary"
                },
                {
                  "type": "null"
                }
              ]
            },
            "sender": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "register_tokens"
      ],
      "properties": {
        "register_tokens": {
          "type": "object",
          "required": [
            "tokens",
            "viewing_key"
          ],
          "properties": {
            "tokens": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/SecretContract"
              }
            },
            "viewing_key": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "rescue_tokens"
      ],
      "properties": {
        "rescue_tokens": {
          "type": "object",
          "properties": {
            "denom": {
              "type": [
                "string",
                "null"
              ]
            },
            "key": {
              "type": [
                "string",
                "null"
              ]
            },
            "token_address": {
              "anyOf": [
                {
                  "$ref": "#/definitions/HumanAddr"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "update_addresses_allowed_to_fill"
      ],
      "properties": {
        "update_addresses_allowed_to_fill": {
          "type": "object",
          "required": [
            "addresses_allowed_to_fill"
          ],
          "properties": {
            "addresses_allowed_to_fill": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/HumanAddr"
              }
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "Binary": {
      "description": "Binary is a wrapper around Vec<u8> to add base64 de/serialization with serde. It also adds some helper methods to help encode inline.\n\nThis is only needed as serde-json-{core,wasm} has a horrible encoding for Vec<u8>",
      "type": "string"
    },
    "Hop": {
      "type": "object",
      "required": [
        "from_token",
        "trade_smart_contract"
      ],
      "properties": {
        "from_token": {
          "$ref": "#/definitions/SecretContract"
        },
        "position": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint32",
          "minimum": 0.0
        },
        "trade_smart_contract": {
          "$ref": "#/definitions/SecretContract"
        }
      }
    },
    "HumanAddr": {
      "type": "string"
    },
    "SecretContract": {
      "type": "object",
      "required": [
        "address",
        "contract_hash"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "contract_hash": {
          "type": "string"
        }
      }
    },
    "Uint128": {
      "type": "string"
    }
  }
}
